<section id="administrator-show" class="container">
	<div class="page-header">
		<h1>Show <strong><%= @administrator.user_name %></strong> </h1>
	</div>

	<div class="form-group">
		<img src="#">
	</div>

	<div class="form-group">
		<p> <strong>Full Name:</strong><br>
			<%= @administrator.full_name %>
		</p>
	</div>

	<div class="form-group">
		<p> <strong>User name:</strong><br>
			<%= @administrator.user_name %>
		</p>
	</div>

	<div class="form-group">
		<p> <strong>E-mail:</strong><br>
			<%= @administrator.user.email %>
		</p>
	</div>

	<div class="form-group">
		<p> <strong>created at:</strong><br>
			<%= @administrator.created_at %>
		</p>
	</div>

	<div class="form-group">
		<p> <strong>Updated at:</strong><br>
			<%= @administrator.updated_at %>
		</p>
	</div>

	<%= link_to :Edit, edit_administrator_path, :class => "btn btn-warning btn-lg" %>
	<%= link_to :Back, administrators_path, :class => "btn btn-danger btn-lg" %>
</section>